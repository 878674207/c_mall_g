<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.ruoyi.tob.mapper.ProductAttributeMapper">
    <update id="updateAttributeToNull">
        update pms_product_attribute
        set hand_add_status = null,
        select_type = null,
        input_list = null
        where id = #{id}
    </update>


</mapper>