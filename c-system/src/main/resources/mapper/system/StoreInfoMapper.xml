<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.ruoyi.system.mapper.StoreInfoMapper">

    <select id="queryStoreIdByuserAccount" parameterType="java.lang.String" resultType="java.lang.Long">
        select id from store_info where store_account = #{userAccount} limit 1
    </select>

</mapper>